{
  "expo": {
    "name": "MDNT BRWK",
    "slug": "midnight-breath",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000000",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "bundleIdentifier": "com.mdnt.brwk",
      "buildNumber": "12",
      "appleTeamId": "QTZ99Y3958",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "This app does not use the microphone. This permission is required by an audio playback library.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsLocalNetworking": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#000000"
      },
      "newArchEnabled": true,
      "package": "com.mdnt.brwk"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-splash-screen",
      ["react-native-audio-api", { "iosBackgroundMode": true }]
    ],
    "fonts": [
      {
        "asset": "./assets/fonts/Inter-Regular.ttf",
        "family": "Inter"
      },
      {
        "asset": "./assets/fonts/Inter-Bold.ttf",
        "family": "Inter-Bold"
      }
    ]
  }
}
